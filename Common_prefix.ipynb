        "list1 = [\"flower\", \"flow\", \"flight\", \"flew\"]\n",
        "list1.sort()\n",
        "left, right = list1[0], list1[-1]\n",
        "i = 0\n",
        "while i < len(left) and i < len(right):\n",
        "  if left[i] != right[i]:\n",
        "    break\n",
        "  i+=1\n",
        "if(i>0):\n",
        "  print(f\"common prefix: {left[:i]}\")\n",
        "else:\n",
        "  print(f\"common prefix: \")"
      ]
